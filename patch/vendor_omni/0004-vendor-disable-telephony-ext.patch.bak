From d78de529840d610b55c110918ac62737fe453f25 Mon Sep 17 00:00:00 2001
From: maxwen <max.weninger@gmail.com>
Date: Sat, 16 Sep 2017 02:38:07 +0200
Subject: [PATCH 4/4] vendor: disable telephony-ext

Change-Id: Ia7aebb49a2de88c609252eaf6bee5b3cea9ddfb8
---
 config/packages.mk                         | 4 ++--
 utils/emulator/skins/pixel_xl/hardware.ini | 2 +-
 utils/emulator/start_emulator_image.sh     | 2 +-
 3 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/config/packages.mk b/config/packages.mk
index 669c4e3..ffe70da 100644
--- a/config/packages.mk
+++ b/config/packages.mk
@@ -34,8 +34,8 @@ PRODUCT_PACKAGES += \
     fsck.ntfs
 
 # Telephony extension
-PRODUCT_PACKAGES += telephony-ext
-PRODUCT_BOOT_JARS += telephony-ext
+#PRODUCT_PACKAGES += telephony-ext
+#PRODUCT_BOOT_JARS += telephony-ext
 
 # OMS support
 #PRODUCT_PACKAGES += ThemeInterfacer \
diff --git a/utils/emulator/skins/pixel_xl/hardware.ini b/utils/emulator/skins/pixel_xl/hardware.ini
index eeaae5c..3c9eae8 100644
--- a/utils/emulator/skins/pixel_xl/hardware.ini
+++ b/utils/emulator/skins/pixel_xl/hardware.ini
@@ -44,7 +44,7 @@ kernel.supportsYaffs2 = no
 disk.vendorPartition.size = 0
 hw.sensors.magnetic_field_uncalibrated = true
 hw.sensors.gyroscope_uncalibrated = true
-hw.sdCard = false
+hw.sdCard = true
 hw.gyroscope = true
 runtime.network.latency=none
 runtime.network.speed=full
diff --git a/utils/emulator/start_emulator_image.sh b/utils/emulator/start_emulator_image.sh
index 022c4c5..4f6db8a 100755
--- a/utils/emulator/start_emulator_image.sh
+++ b/utils/emulator/start_emulator_image.sh
@@ -4,4 +4,4 @@
 export ANDROID_BUILD_TOP=/tmp/generic_x86
 export ANDROID_PRODUCT_OUT=/tmp/generic_x86
 
-~/Android/Sdk/tools/emulator  -verbose -skindir /tmp/generic_x86/skins/ -skin pixel_xl -kernel ~/Android/Sdk/system-images/android-26/google_apis/x86/kernel-ranchu -gpu host -ramdisk ~/Android/Sdk/system-images/android-26/google_apis/x86/ramdisk.img
+~/Android/Sdk/tools/emulator  -verbose -skindir /tmp/generic_x86/skins/ -skin pixel_xl -kernel ~/Android/Sdk/system-images/android-26/google_apis/x86/kernel-ranchu -gpu host -ramdisk ~/Android/Sdk/system-images/android-26/google_apis/x86/ramdisk.img -sdcard ~/Android/Sdk/system-images/android-26/google_apis/x86/sdcard.img
